├── dist # 构建产物
│   ├── client # client bundle
│   └── server # server bundle
├── public 
├── src
│   ├── router
│   ├────── index.js # 路由声明
│   ├── store
│   ├────── index.js # 全局状态
│   ├── pages
│   ├────── home.vue
│   ├────── user.vue
│   ├────── setting.vue
│   ├── App.vue # 应用的根节点
│   ├── entry-client.js # 客户端⼊⼝，⽤于静态内容“激活”
│   └── entry-server.js # 服务端⼊⼝，⽤于⾸屏内容渲染
├── .env.client
├── .env.server
├── package.json
├── package-lock.json
├── serve.js # node服务启动文件
